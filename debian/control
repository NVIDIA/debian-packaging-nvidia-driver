#Package: ${nvidia}-kernel-dkms
Package: nvidia-kernel-dkms
Section: non-free/kernel
Architecture: i386 amd64
Multi-Arch: foreign
Pre-Depends:
 nvidia-installer-cleanup,
Depends:
 dkms,
 ${nvidia}-kernel-support--v1,
 ${misc:Depends}
