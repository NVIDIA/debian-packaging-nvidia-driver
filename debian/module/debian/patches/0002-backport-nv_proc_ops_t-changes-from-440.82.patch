+static const nv_proc_ops_t nv_procfs_offline_pages_fops = {
+    NV_PROC_OPS_SET_OWNER()
+    .NV_PROC_OPS_OPEN    = nv_procfs_open_offline_pages,
+    .NV_PROC_OPS_READ    = seq_read,
